 <div class="header">
<div class="hero-image">
  </div>
 

<div class="row row-cols-1 row-cols-md-2 g-4">
  <div class="col">
  {{#each allExercises}}
  <div class="card" style="width: 18rem;">
  <img src="{{image}}" class="card-img-top" alt="squat">
  <div class="card-body p-3">
    <h2 class="card-title">{{name}}</h2>
    <p class="card-text">{{description}}</p>
    <p class="card-text">{{funny_quip}}</p>
    <button id="saveFav" class="btn btn-primary" value="{{id}}">Save to Favorites</button>
  </div>
</div>
{{/each}}
</div>
</div>

  
  <script src="/js/save-to-favorites.js"></script>
  
