 <div class="hero-image"></div>
 

<div class="row">
  {{#each allExercises}}
  <div class="card" style="width: 18rem;">
  <img src="/images/{{name}}" class="card-img-top" alt="squat">
  <div class="card-body p-3">
    <h5 class="card-title">{{name}}</h5>
    <p class="card-text">{{description}}</p>
    <p class="card-text">{{funny_quip}}</p>
    <button id="saveFav" class="btn btn-primary" value="{{id}}">Save to Favorites</button>
  </div>
</div>
{{/each}}
</div>
  
  <script src="/js/save-to-favorites.js"></script>
  
